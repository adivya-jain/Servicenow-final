        <messages>An onSubmit client script to validate that the end_time field is not before the start_time in the Schedule form</messages>
        <name>Validate End Date</name>
        <order/>
        <script><![CDATA[function onSubmit() {
	var startDate = g_form.getValue('x_start_date');
	var endDate = g_form.getValue('x_end_date');
	if(new Date(startDate) >= new Date(endDate)){
		g_form.addErrorMessage('Invalid end date');
		return false;
	}
	
}]]></script>
